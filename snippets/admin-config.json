{
  "admin search config":{
    "scope": "typescript,typescriptreact",
    "prefix": "admin.searchConfig",
    "description": "list列和搜索项配置",
    "body":[
      "import { TableColumnData } from '@arco-design/web-vue/es/table/interface.d';",
      "import { FormFiled } from '@xier/admin-ui/es/form/src/props';",
      "",
      "export const getFormFileds = (): FormFiled[] => {",
      "const formBaseList: FormFiled[] = [",
      "  {",
      "    label: '编号',",
      "    filed: 'channelCodeLv1',",
      "    props: {",
      "      col: {",
      "        span: 6,",
      "      },",
      "      component: {",
      "        placeholder: '请输入编号',",
      "        style: {",
      "          width: '100%',",
      "        },",
      "      },",
      "    },",
      "    component: 'a-input',",
      "  },",
      "  {",
      "    label: '创建时间',",
      "    filed: 'date',",
      "    props: {",
      "      col: {",
      "        span: 12,",
      "      },",
      "      component: {",
      "        format: 'YYYY-MM-DD',",
      "        style: {",
      "          width: '100%',",
      "        },",
      "      },",
      "    },",
      "    component: 'a-range-picker',",
      "  },",
      "];",
      "",
      "return formBaseList;",
    "};",
    "",
    "export function getBasicColumns(): TableColumnData[] {",
    "  const arr: TableColumnData[] = [",
    "    {",
    "      dataIndex: 'no',",
    "      title: '序号',",
    "      fixed: 'left',",
    "      align: 'center',",
    "      width: 60,",
    "    },",
    "    {",
    "      dataIndex: 'action',",
    "      title: '操作',",
    "      fixed: 'right',",
    "      align: 'center',",
    "      width: 100,",
    "      slotName: 'action',",
    "    },",
    "  ];",
    "",
    "  return arr;",
    "}",
    ""
    ]
  },
  "router default config":{
    "scope": "typescript",
    "prefix": "admin.router",
    "description": "路由基本结构片断",
    "body":[
        "export default {",
        "    path: '${1}',",
        "    name: '${2}',",
        "    component: () => import('@/layout/page-layout.vue'),",
        "    meta: {",
        "        title: '${3}',",
        "        icon: '${4}',",
        "    },",
        "    children: [",
        "      {",
        "        path: '${5}',",
        "        name: '${6}',",
        "        component: () => import('@/views/${7}/index.vue'),",
        "        meta: {",
        "            title: '标题',",
        "            cacheName: '缓存组件名称',",
        "        },",
        "      }",
        "    ],",
        "};"
    ]
  },
